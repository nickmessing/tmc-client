    "mkdirp": "^1.0.4",
    "openapi-typescript": "^4.0.2",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass": "^1.37.5",
    "sass-loader": "^10",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "vue-cli-plugin-tailwind": "^2.0.6",
    "vue-jest": "^5.0.0-alpha.10"
  }
