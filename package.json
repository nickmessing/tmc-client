{
  "name": "tmc-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "generate:openapi": "openapi-typescript http://localhost:8888/api-json -o src/api/generated/openapi.ts",
    "generate:fix": "ts-node --project ./tsconfig.node.json scripts/fix-types",
    "generate:clear": "rimraf src/api/generated && mkdirp src/api/generated",
    "generate": "yarn generate:clear && yarn generate:openapi && yarn generate:fix",
    "postgenerate": "prettier -w src/api/generated"
  },
  "dependencies": {
    "@mdi/js": "^5.9.55",
    "@tailwindcss/postcss7-compat": "^2.2.7",
    "autoprefixer": "^9",
